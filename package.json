{
  "name": "feedflow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "TURBOPACK=0 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "docker-build": "docker build --build-arg DATABASE_URL=postgresql://feedflow:feedflow@localhost:5432/feedflow?schema=public -t dovics1/feedflow:$(git describe --tags --dirty --match='v*') .",
    "docker-build-external": "docker build --build-arg DATABASE_URL=${DATABASE_URL:-postgresql://user:password@host:5432/feedflow?schema=public} -t dovics1/feedflow:$(git describe --tags --dirty --match='v*') .",
    "docker-push": "docker push dovics1/feedflow:$(git describe --tags --dirty --match='v*')",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/client": "^6.19.1",
    "bcryptjs": "^3.0.3",
    "dotenv": "^16.4.7",
    "fast-xml-parser": "^5.3.3",
    "next": "16.1.1",
    "next-auth": "^4.24.13",
    "next-intl": "^4.7.0",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "prisma": "^6.19.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0",
    "rss-parser": "^3.13.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
